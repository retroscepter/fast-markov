  "napi": {
    "name": "fast-markov",
    "package": {
      "name": "@fast-markov/native"
    },
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-pc-windows-msvc"
      ]
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/retroscepter/fast-markov"
  },
  "devDependencies": {
